{
  "name": "freecom",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "dev": "nx run-many --target serve --projects docs-samples-signaling,docs-samples-peer-client --maxParallel 2 --parallel",
    "start": "node dist/apps/api/gateway/main.js",
    "gateway": "nx run api-gateway:build --configuration=production --with-deps",
    "build:signaling": "nx run docs-samples-signaling:build --prod --with-deps",
    "signaling": "node dist/apps/docs/samples/signaling/main.js",
    "task": "ts-node --project tools/tsconfig.tools.json tools/tasks/init-task.ts",
    "build": "nx affected:build --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "prerelease": "nx affected:build --all --prod",
    "release": "nx affected --target release --all",
    "commit": "git-cz",
    "help": "nx help"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/guiseek/freecom.git"
  },
  "author": {
    "name": "Guilherme Siquinelli",
    "email": "email@guiseek.dev",
    "url": "https://guiseek.dev"
  },
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/cdk": "^11.0.2",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "@nestjs/common": "^7.6.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.1",
    "@nestjs/mapped-types": "^0.1.1",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/platform-socket.io": "^7.6.1",
    "@nestjs/websockets": "^7.6.1",
    "@nrwl/angular": "10.4.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "immutable": "^4.0.0-rc.12",
    "mongoose": "^5.11.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "socket.io-client": "^2.3.1",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "10.4.4",
    "@nrwl/cypress": "10.4.4",
    "@nrwl/eslint-plugin-nx": "10.4.4",
    "@nrwl/jest": "10.4.4",
    "@nrwl/nest": "10.4.4",
    "@nrwl/node": "10.4.4",
    "@nrwl/tao": "10.4.4",
    "@nrwl/workspace": "10.4.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@trellisorg/make-buildable": "0.0.8",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@types/socket.io": "^2.1.12",
    "@types/socket.io-client": "^1.4.34",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.1",
    "commitizen": "^4.2.2",
    "cypress": "^5.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^4.3.6",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "^10.5.3",
    "listr2": "^3.2.3",
    "ng-packagr": "^10.1.0",
    "prettier": "2.1.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add ."
    ]
  }
}
